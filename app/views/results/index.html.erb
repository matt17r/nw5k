<h1>Results - Event <%= @event %></h1>

<div id="results">
  <% @results.each do |result| %>
    <%= render result %>
    <p>
      <%= link_to "Edit this result", edit_event_result_path(result.event, result) %>
    </p>
  <% end %>
</div>

<%= link_to "New result", new_event_result_path %>
